    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "Agency_Sensor_MAIN.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "qt": "pyqt5"
            
        },
        {
            "name": "Agency_Sensor_MAIN: hebrew",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/Agency_Sensor_MAIN.py",
            "args": [
                "hebrew"
            ],
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "justMyCode": false,
            "qt": "pyqt5"
        },
        {
            "name": "Agency_Sensor_MAIN: with argumentâ€¦",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/Agency_Sensor_MAIN.py",
            "args": [
                "${input:agencyArg}"
            ],
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "justMyCode": false,
            "qt": "pyqt5"
        }
    ],
    "inputs": [
        {
            "id": "agencyArg",
            "type": "promptString",
            "description": "Argument to pass to Agency_Sensor_MAIN.py",
            "default": "hebrew"
        }
    ]
}